    <script>
jQuery(document).ready(function ($) {
    size_li = $("#myList.directoryList > li").size();
    x=8;
    $('#myList.directoryList > li:lt('+x+')').show();
    $('#loadMore').click(function () {
        x= (x+3 <= size_li) ? x+3 : size_li;
        $('#myList.directoryList > li:lt('+x+')').slideDown();
         $('#showLess').show();
        if(x == size_li){
            $('#loadMore').hide();
        }
    });
    $('#showLess').click(function () {
        x=(x-3<0) ? 3 : x-3;
        $('#myList.directoryList > li').not(':lt('+x+')').slideUp();
        $('#loadMore').show();
         $('#showLess').show();
        if(x == 3){
            $('#showLess').hide();
        }
    });
});
    </script>


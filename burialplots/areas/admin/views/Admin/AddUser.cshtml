
@{
    ViewBag.Title = "Add User";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
@section Scripts{


    
<script src="~/Areas/Admin/JS/member.js"></script>


}
<h2>Add User</h2>
<div class="row">
    <div class="col-md-12">
        <table class="table table-hover table-hover">
            <tr>
                <td>First Name</td>
                <td><input type="text" id="fname" class="form-control" /></td>
            </tr>
            <tr>
                <td>
                    Last Name
                </td>
                <td>
                    <input type="text" id="lname" class="form-control" />
                </td>
            </tr>
            <tr>
                <td>
                    UserName
                </td>
                <td>
                    <input type="text" id="usename" class="form-control" />
                </td>
            </tr>
            <tr>
                <td>
                    Email
                </td>
                <td>
                    <input type="email" id="email" class="form-control" />
                </td>
            </tr>
            <tr>
                <td>
                    Cell Number
                </td>
                <td>
                    <input type="text" id="cell" class="form-control" />
                </td>
            </tr>
            <tr>
                <td>Password</td>
                <td>
                    <input type="password" id="pass" class="form-control" />
                </td>
            </tr>
            <tr>
                <td>
                    Select Role Type
                </td>
                <td>
                    <select class="form-control" id="rooltype">
                        <option value="SuperAdmin">SuperAdmin</option>
                        <option value="Admin">Admin</option>
                        <option value="Partner">Partner</option>
                        <option value="User">User</option>
                   
                    </select>
                </td>
            </tr>




        </table>
 
    </div>
    
</div>
<div class="row">
    <div class="col-md-10">

    </div>
    <div class="col-md-2">
        <input type="button" value="Submit" onclick="addnewuser()" class="form-control btn-primary pull-right" />
    </div>
</div>

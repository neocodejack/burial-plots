        <li>
            <span><strong>Member Registration</strong></span>
        </li>
    </ul>
</div>

<h3 class="page-title">
    <strong>Registration</strong>
</h3>



    <div class="row">
        <h4>Company Information</h4>
        <div class="col-md-6">
            <div class="form-group form-md-line-input form-md-floating-label">
                <input type="text" class="form-control InputRequired save clear" name="CompanyName" id="fname" value="">
                <label for="txtcompanyname">First Name*</label>
            </div>
        </div>
     

    </div>

    <div class="row">
    <div class="col-md-6">
        <div class="form-group form-md-line-input form-md-floating-label">
            <input type="text" class="form-control InputRequired save clear" name="FirstName" id="lname" value="">
            <label for="txtLastName">Last Name*</label>
        </div>
    </div>
</div>
        <div class="row">
        <div class="col-md-6">
            <div class="form-group form-md-line-input form-md-floating-label">
                <input type="text" class="form-control InputRequired save clear" name="LastName" id="email" value="">
                <label for="txtLastName">Email*</label>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-6">
            <div class="form-group form-md-line-input form-md-floating-label">
                <input type="text" class="form-control InputRequired save clear" name="UserName" id="dob" value="">
                <label for="txtLastName">Date Of Birth*</label>
            </div>
        </div>
        @*<div class="col-md-4">
            <div class="form-group form-md-line-input form-md-floating-label">
                <input type="password" class="form-control InputRequired save clear" name="Password" id="txtPassword" value="">
                <label for="Password">Password*</label>
            </div>
        </div>*@
     

    <div class="row">
        <div class="col-md-8">
            <div class="form-actions noborder">
                <button type="button" class="btn blue" id="btnSave" onclick="Save()">Save</button>
                <button type="button" class="btn default" onclick="ClearAll()">Reset</button>
            </div>
        </div>
    </div>
</div>


